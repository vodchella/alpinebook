					"event": [
						{
							"listen": "prerequest",
							"script": {
								"type": "text/javascript",
								"exec": [
									"const postRequest = {",
									"    url: pm.environment.get(\"alpinebook-server\") + \"/users/signin/111111111?method=telegram\",",
									"    method: \"POST\"",
									"}",
									"pm.sendRequest(postRequest, function (err, response) {",
									"    resp = response.json()",
									"    jwt = resp[\"jwt\"]",
									"    pm.globals.set(\"current_jwt\", resp[\"jwt\"]);",
									"});"
								]
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": [
